{
  "name": "@bakes/dastardly-root",
  "version": "0.1.0",
  "description": "High-performance data format parser and serializer with common AST",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "typecheck": "pnpm -r typecheck",
    "clean": "pnpm -r exec rm -rf dist node_modules",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "benchmark": "pnpm benchmark:json && pnpm benchmark:yaml && pnpm benchmark:csv && pnpm benchmark:validation",
    "benchmark:json": "pnpm --filter @bakes/dastardly-json benchmark",
    "benchmark:yaml": "pnpm --filter @bakes/dastardly-yaml benchmark",
    "benchmark:csv": "pnpm --filter @bakes/dastardly-csv benchmark",
    "benchmark:validation": "pnpm --filter @bakes/dastardly-validation benchmark"
  },
  "keywords": [
    "parser",
    "serializer",
    "ast",
    "json",
    "yaml",
    "xml",
    "csv",
    "toml",
    "tree-sitter",
    "validation"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^24.10.0",
    "prettier": "^3.1.0",
    "semantic-release": "^23.0.0",
    "semantic-release-monorepo": "^8.0.2",
    "typescript": "^5.3.0"
  }
}
