{
  "name": "@bakes/dastardly-csv",
  "version": "0.1.0",
  "description": "CSV parser and serializer for dASTardly",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "benchmark": "pnpm build && npx tsx benchmarks/run.ts",
    "prepublishOnly": "pnpm build && pnpm test"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "keywords": [
    "csv",
    "tsv",
    "psv",
    "parser",
    "serializer",
    "ast",
    "tree-sitter"
  ],
  "author": "The Software Bakery",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thesoftwarebakery/dastardly.git",
    "directory": "packages/csv"
  },
  "homepage": "https://github.com/thesoftwarebakery/dastardly#readme",
  "bugs": "https://github.com/thesoftwarebakery/dastardly/issues",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@bakes/dastardly-core": "workspace:^",
    "@bakes/dastardly-tree-sitter-csv": "workspace:^",
    "@bakes/dastardly-tree-sitter-runtime": "workspace:^",
    "tree-sitter": "^0.21.1"
  },
  "devDependencies": {
    "@types/benchmark": "^2.1.5",
    "@types/node": "^24.10.0",
    "@vitest/ui": "^1.6.1",
    "benchmark": "^2.1.4",
    "csv-parse": "^5.6.0",
    "csv-stringify": "^6.5.2",
    "typescript": "^5.3.0",
    "vitest": "^1.6.1"
  }
}
