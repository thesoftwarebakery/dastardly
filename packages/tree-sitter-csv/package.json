{
  "name": "@bakes/dastardly-tree-sitter-csv",
  "version": "1.2.0",
  "description": "CSV, PSV, & TSV grammar for tree-sitter (dASTardly fork with prebuilt binaries)",
  "main": "bindings/node",
  "files": [
    "binding.gyp",
    "bindings/node/*",
    "common/**",
    "csv/grammar.js",
    "csv/src/**",
    "psv/grammar.js",
    "psv/src/**",
    "tsv/grammar.js",
    "tsv/src/**",
    "src/**"
  ],
  "keywords": [
    "parsing",
    "incremental",
    "csv",
    "psv",
    "tsv"
  ],
  "author": "The Software Bakery (original by Amaan Qureshi <amaanq12@gmail.com>)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thesoftwarebakery/dastardly.git",
    "directory": "packages/tree-sitter-csv"
  },
  "homepage": "https://github.com/thesoftwarebakery/dastardly#readme",
  "bugs": "https://github.com/thesoftwarebakery/dastardly/issues",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "node-addon-api": "^8.2.2",
    "node-gyp-build": "^4.8.2"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "devDependencies": {
    "eslint": "^8.47.0",
    "eslint-config-google": "^0.14.0",
    "prebuildify": "^6.0.1"
  },
  "scripts": {
    "generate": "npm run build-csv && npm run build-psv && npm run build-tsv",
    "build-csv": "cd csv && tree-sitter generate",
    "build-psv": "cd psv && tree-sitter generate",
    "build-tsv": "cd tsv && tree-sitter generate",
    "install": "node-gyp-build",
    "prebuildify": "prebuildify --napi --strip",
    "lint": "eslint common/define-grammar.js csv/grammar.js tsv/grammar.js",
    "test-load": "node -e \"console.log(require('./bindings/node').csv.name, require('./bindings/node').psv.name, require('./bindings/node').tsv.name)\"",
    "test": "npm run test-load"
  },
  "tree-sitter": [
    {
      "scope": "source.csv",
      "file-types": [
        "csv"
      ],
      "path": "csv",
      "highlights": [
        "queries/highlights.scm"
      ],
      "injection-regex": "csv"
    },
    {
      "scope": "source.psv",
      "file-types": [
        "psv"
      ],
      "path": "psv",
      "highlights": [
        "queries/highlights.scm"
      ],
      "injection-regex": "psv"
    },
    {
      "scope": "source.tsv",
      "file-types": [
        "tsv"
      ],
      "path": "tsv",
      "highlights": [
        "queries/highlights.scm"
      ],
      "injection-regex": "tsv"
    }
  ]
}
