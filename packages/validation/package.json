{
  "name": "@bakes/dastardly-validation",
  "version": "0.1.0",
  "description": "JSON Schema validator for dASTardly ASTs",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "benchmark": "pnpm build && npx tsx benchmarks/run.ts",
    "prepublishOnly": "pnpm build && pnpm test"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "keywords": [
    "json-schema",
    "validator",
    "validation",
    "ast"
  ],
  "author": "The Software Bakery",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/thesoftwarebakery/dastardly.git",
    "directory": "packages/validation"
  },
  "homepage": "https://github.com/thesoftwarebakery/dastardly#readme",
  "bugs": "https://github.com/thesoftwarebakery/dastardly/issues",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@bakes/dastardly-core": "workspace:^",
    "@types/json-schema": "^7.0.15"
  },
  "devDependencies": {
    "@bakes/dastardly-json": "workspace:^",
    "@json-schema-org/tests": "^1.0.0",
    "@types/benchmark": "^2.1.5",
    "@vitest/ui": "^1.6.1",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "benchmark": "^2.1.4",
    "typescript": "^5.3.0",
    "vitest": "^1.6.1"
  }
}
